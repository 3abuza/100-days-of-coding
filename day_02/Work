# a = int("five")
# print(type(a))
print("Welcome to the tip calculator.")
bill = float(input("What was the total bill? $"))
print(bill)
split_bill = input("How many people to split the bill? ")
print(split_bill)
percenage = input("What percentage tip would you like to give? 10, 12, or 15? ")
each_pay = str(bill/split_bill(percenage*100))
print("Each person should pay: $" + each_pay)